_file":1291,"_extension":1292},"leetcode",false,"","数字 n 代表生成括号的对数，请你设计一个函数，用于能够生成所有可能的并且 有效的 括号组合。","https://leetcode.cn/problems/generate-parentheses",{"type":412,"children":413,"toc":1282},"root",[414,421,426,456,461,484,489,499,505,510,523,528,534,1276],{"type":415,"tag":416,"props":417,"children":418},"element","p",{},[419],{"type":420,"value":409},"text",{"type":415,"tag":416,"props":422,"children":423},{},[424],{"type":420,"value":425},"示例 1：",{"type":415,"tag":427,"props":428,"children":431},"pre",{"className":429,"code":430,"language":420,"meta":408,"style":408},"language-text shiki shiki-themes slack-ochin slack-ochin ayu-dark","输入：n = 3\n输出：[\"((()))\",\"(()())\",\"(())()\",\"()(())\",\"()()()\"]\n",[432],{"type":415,"tag":433,"props":434,"children":435},"code",{"__ignoreMap":408},[436,447],{"type":415,"tag":437,"props":438,"children":441},"span",{"class":439,"line":440},"line",1,[442],{"type":415,"tag":437,"props":443,"children":444},{},[445],{"type":420,"value":446},"输入：n = 3\n",{"type":415,"tag":437,"props":448,"children":450},{"class":439,"line":449},2,[451],{"type":415,"tag":437,"props":452,"children":453},{},[454],{"type":420,"value":455},"输出：[\"((()))\",\"(()())\",\"(())()\",\"()(())\",\"()()()\"]\n",{"type":415,"tag":416,"props":457,"children":458},{},[459],{"type":420,"value":460},"示例 2：",{"type":415,"tag":427,"props":462,"children":464},{"className":429,"code":463,"language":420,"meta":408,"style":408},"输入：n = 1\n输出：[\"()\"]\n",[465],{"type":415,"tag":433,"props":466,"children":467},{"__ignoreMap":408},[468,476],{"type":415,"tag":437,"props":469,"children":470},{"class":439,"line":440},[471],{"type":415,"tag":437,"props":472,"children":473},{},[474],{"type":420,"value":475},"输入：n = 1\n",{"type":415,"tag":437,"props":477,"children":478},{"class":439,"line":449},[479],{"type":415,"tag":437,"props":480,"children":481},{},[482],{"type":420,"value":483},"输出：[\"()\"]\n",{"type":415,"tag":416,"props":485,"children":486},{},[487],{"type":420,"value":488