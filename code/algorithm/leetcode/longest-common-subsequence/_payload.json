":329,"value":344},"示例 1：",{"type":324,"tag":346,"props":347,"children":351},"pre",{"className":348,"code":349,"language":350,"meta":317,"style":317},"language-md shiki shiki-themes slack-ochin slack-ochin ayu-dark","输入：text1 = \"abcde\", text2 = \"ace\"\n输出：3\n解释：最长公共子序列是 \"ace\" ，它的长度为 3 。\n","md",[352],{"type":324,"tag":353,"props":354,"children":355},"code",{"__ignoreMap":317},[356,368,377],{"type":324,"tag":357,"props":358,"children":361},"span",{"class":359,"line":360},"line",1,[362],{"type":324,"tag":357,"props":363,"children":365},{"style":364},"--shiki-light:#110000;--shiki-default:#110000;--shiki-dark:#BFBDB6",[366],{"type":329,"value":367},"输入：text1 = \"abcde\", text2 = \"ace\"\n",{"type":324,"tag":357,"props":369,"children":371},{"class":359,"line":370},2,[372],{"type":324,"tag":357,"props":373,"children":374},{"style":364},[375],{"type":329,"value":376},"输出：3\n",{"type":324,"tag":357,"props":378,"children":380},{"class":359,"line":379},3,[381],{"type":324,"tag":357,"props":382,"children":383},{"style":364},[384],{"type":329,"value":385},"解释：最长公共子序列是 \"ace\" ，它的长度为 3 。\n",{"type":324,"tag":325,"props":387,"children":388},{},[389],{"type":329,"value":390},"示例 2：",{"type":324,"tag":346,"props":392,"children":394},{"className":348,"code":393,"language":350,"meta":317,"style":317},"输入：text1 = \"abc\", text2 = \"abc\"\n输出：3\n解释：最长公共子序列是 \"abc\" ，它的长度为 3 。\n",[395],{"type":324,"tag":353,"props":396,"children":397},{"__ignoreMap":317},[398,406,413],{"type":324,"tag":357,"props":399,"children":400},{"class":359,"line":360},[401],{"type":324,"tag":357,"props":402,"children":403},{"style":364},[404],{"type":329,"value":405},"输入：text1 = \"abc\", text2 = \"abc\"\n",{"type":324,"tag":357,"props":407,"children":408},{"class":359,"line":370},[409],{"type":324,"tag":357,"props":410,"children":411},{"style":364},[412],{"type":329,"value":376},{"type":324,"tag":357,"props":414,"children":415},{"class":359,"line":379},[416],{"type":324,"tag":357,"props":417,"children":418}