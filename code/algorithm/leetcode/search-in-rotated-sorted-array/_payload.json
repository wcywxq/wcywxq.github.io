 ..., nums[k-1]]（下标 从 0 开始 计数）。例如， [0,1,2,4,5,6,7] 在下标 3 处经旋转后可能变为 [4,5,6,7,0,1,2] 。",{"type":330,"tag":331,"props":342,"children":343},{},[344],{"type":335,"value":345},"给你 旋转后 的数组 nums 和一个整数 target ，如果 nums 中存在这个目标值 target ，则返回它的下标，否则返回 -1 。",{"type":330,"tag":331,"props":347,"children":348},{},[349],{"type":335,"value":350},"你必须设计一个时间复杂度为 O(log n) 的算法解决此问题。",{"type":330,"tag":331,"props":352,"children":353},{},[354],{"type":335,"value":355},"示例 1：",{"type":330,"tag":357,"props":358,"children":361},"pre",{"className":359,"code":360,"language":335,"meta":323,"style":323},"language-text shiki shiki-themes slack-ochin slack-ochin ayu-dark","输入：nums = [4,5,6,7,0,1,2], target = 0\n输出：4\n",[362],{"type":330,"tag":363,"props":364,"children":365},"code",{"__ignoreMap":323},[366,377],{"type":330,"tag":367,"props":368,"children":371},"span",{"class":369,"line":370},"line",1,[372],{"type":330,"tag":367,"props":373,"children":374},{},[375],{"type":335,"value":376},"输入：nums = [4,5,6,7,0,1,2], target = 0\n",{"type":330,"tag":367,"props":378,"children":380},{"class":369,"line":379},2,[381],{"type":330,"tag":367,"props":382,"children":383},{},[384],{"type":335,"value":385},"输出：4\n",{"type":330,"tag":331,"props":387,"children":388},{},[389],{"type":335,"value":390},"示例 2：",{"type":330,"tag":357,"props":392,"children":394},{"className":359,"code":393,"language":335,"meta":323,"style":323},"输入：nums = [4,5,6,7,0,1,2], target = 3\n输出：-1\n",[395],{"type":330,"tag":363,"props":396,"children":397},{"__ignoreMap":323},[398,406],{"type":330,"tag":367,"props":399,"children":400},{"class":369,"line":370},[401],{"type":330,"tag":367,"props":402,"children":403},{},[404],{"type":335,"value":405},"输入：nums = [4,5,6,7,0,1,2], target = 3\n",{"type":330,"tag":367,"props":407,"children":408},{"class":369,"line":379},[409],{"type":330,"tag":367,"props":410,"children":411},{},[412],{"type":335,"value":413},"输出：-1\n",{"type":330,"tag":331,"props":415,"children":416},{},[417],{"type":335,"value":418},"示例 3：",{"type":330,"tag":357,"props":420,"children":422},{"className":359,"code":421,"language":335,"meta":323,"style":323},"输入：nums = [1], target = 0\n输出：-1\n",[423],{"type":330,"tag":363,"props":424,"children":425},{"__ignoreMap":323},[426,434],{"type":330,"tag":367,"props":427,"children":428},{"class":369,"line":370},[429],{"type":330,"tag":367,"props":430,"children":431},{},[432],{"type":335,"value":433},"输入：nums = [1], target = 0\n",{"type":330,"tag":367,"props":435,"children":436},{"class":369,"line":379},[437],{"type":330,"tag":367,"props":438,"children":439},{},[440],{"type":335,"value":413},{"type":330,"tag":331,"props":442,"children":443},{},[444],{"type":335,"value":445},"提示：",{"type":330,"tag":447,"props":448,"children":449},"ul",{},[450,456,461,466,471],{"type":330,"tag":451,"props":452,"children":453},"li",{},[454],{"type":335,"value":455},"1 \u003C= nums.length \u003C= 5000",{"type":330,"tag":451,"props":457,"children":458},{},[459],{"type":335,"value":460},"-10^4 \u003C= nums[i] \u003C= 10^4",{"type":330,"tag":451,"props":462,"children":463},{},[464],{"type":335,"value":465},"nums 中的每个值都 独一无二",{"type":330,"tag":451,"props":467,"children":468},{},[469],{"type":335,"value":470},"题目数据保证 nums 在预先未知的某个下标上进行了旋转",{"type":330,"tag":451,"props":472,"children":473},{},[474],{"type":335,"value":475},"-10^4 \u003C= target \u003C= 10^4",{"type":330,"tag":477,"props":478,"children":480},"h2",{"id":479},"解题思路",[481],{"type":335,"value":479},{"type":330,"tag":477,"props":483,"children":485},{"id":484},"实现",[486],{"type":335,"value":484},{"type":330,"tag":488,"props":489,"children":491},"h3",{"id":490},"二分查找",[492],{"type":335,"value":490},{"type":330,"tag":357,"props":494,"children":498},{"className":495,"code":496,"language":497,"meta":323,"style":323},"language-js shiki shiki-themes slack-ochin slack-ochin ayu-dark","/**\n * @param {number[]} nums\n * @param {number} target\n * @return {number}\n */\nvar search = function (nums, target) {\n  function isBlue(nums, ta