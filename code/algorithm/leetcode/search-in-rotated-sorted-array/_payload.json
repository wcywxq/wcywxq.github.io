":412},2,[414],{"type":363,"tag":400,"props":415,"children":416},{},[417],{"type":368,"value":418},"输出：4\n",{"type":363,"tag":364,"props":420,"children":421},{},[422],{"type":368,"value":423},"示例 2：",{"type":363,"tag":390,"props":425,"children":427},{"className":392,"code":426,"language":368,"meta":356,"style":356},"输入：nums = [4,5,6,7,0,1,2], target = 3\n输出：-1\n",[428],{"type":363,"tag":396,"props":429,"children":430},{"__ignoreMap":356},[431,439],{"type":363,"tag":400,"props":432,"children":433},{"class":402,"line":403},[434],{"type":363,"tag":400,"props":435,"children":436},{},[437],{"type":368,"value":438},"输入：nums = [4,5,6,7,0,1,2], target = 3\n",{"type":363,"tag":400,"props":440,"children":441},{"class":402,"line":412},[442],{"type":363,"tag":400,"props":443,"children":444},{},[445],{"type":368,"value":446},"输出：-1\n",{"type":363,"tag":364,"props":448,"children":449},{},[450],{"type":368,"value":451},"示例 3：",{"type":363,"tag":390,"props":453,"children":455},{"className":392,"code":454,"language":368,"meta":356,"style":356},"输入：nums = [1], target = 0\n输出：-1\n",[456],{"type":363,"tag":396,"props":457,"children":458},{"__ignoreMap":356},[459,467],{"type":363,"tag":400,"props":460,"children":461},{"class":402,"line":403},[462],{"type":363,"tag":400,"props":463,"children":464},{},[465],{"type":368,"value":466},"输入：nums = [1], target = 0\n",{"type":363,"tag":400,"props":468,"children":469},{"class":402,"line":412},[470],{"type":363,"tag":400,"props":471,"children":472},{},[473],{"type":368,"value":446},{"type":363,"tag":364,"props":475,"children":476},{},[477],{"type":368,"value":478},"提示：",{"type":363,"tag":480,"props":481,"children":482},"ul",{},[483,489,494,499,504],{"type":363,"tag":484,"props":485,"children":486},"li",{},[487],{"type":368,"value":488},"1 \u003C= nums.length \u003C= 5000",{"type":363,"tag":484,"props":490,"children":491},{},[492],{"type":368,"value":493},"-10^4 \u003C= nums[i] \u003C= 10^4",{"type":363,"tag":484,"props":495,"children":496},{},[497