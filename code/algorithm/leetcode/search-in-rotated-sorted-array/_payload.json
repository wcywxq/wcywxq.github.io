:414,"children":415},{},[416],{"type":362,"value":417},"示例 2：",{"type":357,"tag":384,"props":419,"children":421},{"className":386,"code":420,"language":362,"meta":350,"style":350},"输入：nums = [4,5,6,7,0,1,2], target = 3\n输出：-1\n",[422],{"type":357,"tag":390,"props":423,"children":424},{"__ignoreMap":350},[425,433],{"type":357,"tag":394,"props":426,"children":427},{"class":396,"line":397},[428],{"type":357,"tag":394,"props":429,"children":430},{},[431],{"type":362,"value":432},"输入：nums = [4,5,6,7,0,1,2], target = 3\n",{"type":357,"tag":394,"props":434,"children":435},{"class":396,"line":406},[436],{"type":357,"tag":394,"props":437,"children":438},{},[439],{"type":362,"value":440},"输出：-1\n",{"type":357,"tag":358,"props":442,"children":443},{},[444],{"type":362,"value":445},"示例 3：",{"type":357,"tag":384,"props":447,"children":449},{"className":386,"code":448,"language":362,"meta":350,"style":350},"输入：nums = [1], target = 0\n输出：-1\n",[450],{"type":357,"tag":390,"props":451,"children":452},{"__ignoreMap":350},[453,461],{"type":357,"tag":394,"props":454,"children":455},{"class":396,"line":397},[456],{"type":357,"tag":394,"props":457,"children":458},{},[459],{"type":362,"value":460},"输入：nums = [1], target = 0\n",{"type":357,"tag":394,"props":462,"children":463},{"class":396,"line":406},[464],{"type":357,"tag":394,"props":465,"children":466},{},[467],{"type":362,"value":440},{"type":357,"tag":358,"props":469,"children":470},{},[471],{"type":362,"value":472},"提示：",{"type":357,"tag":474,"props":475,"children":476},"ul",{},[477,483,488,493,498],{"type":357,"tag":478,"props":479,"children":480},"li",{},[481],{"type":362,"value":482},"1 \u003C= nums.length \u003C= 5000",{"type":357,"tag":478,"props":484,"children":485},{},[486],{"type":362,"value":487},"-10^4 \u003C= nums[i] \u003C= 10^4",{"type":357,"tag":478,"props":489,"children":490},{},[491],{"type":362,"value":492},"nums 中的每个值都 独一无二",{"type":357,"tag":478,"props":494,"children":495},{},[496],{"type":362,"value":497},"题目数据保证