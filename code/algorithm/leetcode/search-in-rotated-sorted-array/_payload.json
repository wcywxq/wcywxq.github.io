], ..., nums[n-1], nums[0], nums[1], ..., nums[k-1]]（下标 从 0 开始 计数）。例如， [0,1,2,4,5,6,7] 在下标 3 处经旋转后可能变为 [4,5,6,7,0,1,2] 。",{"type":332,"tag":333,"props":344,"children":345},{},[346],{"type":337,"value":347},"给你 旋转后 的数组 nums 和一个整数 target ，如果 nums 中存在这个目标值 target ，则返回它的下标，否则返回 -1 。",{"type":332,"tag":333,"props":349,"children":350},{},[351],{"type":337,"value":352},"你必须设计一个时间复杂度为 O(log n) 的算法解决此问题。",{"type":332,"tag":333,"props":354,"children":355},{},[356],{"type":337,"value":357},"示例 1：",{"type":332,"tag":359,"props":360,"children":363},"pre",{"className":361,"code":362,"language":337,"meta":325,"style":325},"language-text shiki shiki-themes slack-ochin slack-ochin ayu-dark","输入：nums = [4,5,6,7,0,1,2], target = 0\n输出：4\n",[364],{"type":332,"tag":365,"props":366,"children":367},"code",{"__ignoreMap":325},[368,379],{"type":332,"tag":369,"props":370,"children":373},"span",{"class":371,"line":372},"line",1,[374],{"type":332,"tag":369,"props":375,"children":376},{},[377],{"type":337,"value":378},"输入：nums = [4,5,6,7,0,1,2], target = 0\n",{"type":332,"tag":369,"props":380,"children":382},{"class":371,"line":381},2,[383],{"type":332,"tag":369,"props":384,"children":385},{},[386],{"type":337,"value":387},"输出：4\n",{"type":332,"tag":333,"props":389,"children":390},{},[391],{"type":337,"value":392},"示例 2：",{"type":332,"tag":359,"props":394,"children":396},{"className":361,"code":395,"language":337,"meta":325,"style":325},"输入：nums = [4,5,6,7,0,1,2], target = 3\n输出：-1\n",[397],{"type":332,"tag":365,"props":398,"children":399},{"__ignoreMap":325},[400,408],{"type":332,"tag":369,"props":401,"children":402},{"class":371,"line":372},[403],{"type":332,"tag":369,"props":404,"children":405},{},[406],{"type":337,"value":407},"输入：nums = [4,5,6,7,0,1,2], target = 3\n",{"type":332,"tag":369,"props":409,"children":410},{"class":371,"line":381},[411],{"type":332,"tag":369,"props":412,"children":413},{},[414],{"type":337,"value":415},"输出：-1\n",{"type":332,"tag":333,"props":417,"children":418},{},[419],{"type":337,"value":420},"示例 3：",{"type":332,"tag":359,"props":422,"children":424},{"className":361,"code":423,"language":337,"meta":325,"style":325},"输入：nums = [1], target = 0\n输出：-1\n",[425],{"type":332,"tag":365,"props":426,"children":427},{"__ignoreMap":325},[428,436],{"type":332,"tag":369,"props":429,"children":430},{"class":371,"line":372},[431],{"type":332,"tag":369,"props":432,"children":433},{},[434],{"type":337,"value":435},"输入：nums = [1], target = 0\n",{"type":332,"tag":369,"props":437,"children":438},{"class":371,"line":381},[439],{"type":332,"tag":369,"props":440,"children":441},{},[442],{"type":337,"value":415},{"type":332,"tag":333,"props":444,"children":445},{},[446],{"type":337,"value":447},"提示：",{"type":332,"tag":449,"props":450,"children":451},"ul",{},[452,458,463,468,473],{"type":332,"tag":453,"props":454,"children":455},"li",{},[456],{"type":337,"value":457},"1 \u003C= nums.length \u003C= 5000",{"type":332,"tag":453,"props":459,"children":460},{},[461],{"type":337,"value":462},"-10^4 \u003C= nums[i] \u003C= 10^4",{"type":332,"tag":453,"props":464,"children":465},{},[466],{"type":337,"value":467},"nums 中的每个值都 独一无二",{"type":332,"tag":453,"props":469,"children":470},{},[471],{"type":337,"value":472},"题目数据保证 nums 在预先未知的某个下标上进行了旋转",{"type":332,"tag":453,"props":474,"children":475},{},[476],{"type":337,"value":477},"-10^4 \u003C= target \u003C= 10^4",{"type":332,"tag":479,"props":480,"children":482},"h2",{"id":481},"解题思路",[483],{"type":337,"value":481},{"type":332,"tag":479,"props":485,"children":487},{"id":486},"实现",[488],{"type":337,"value":486},{"type":332,"tag":490,"props":491,"children":493},"h3",{"id":492},"二分查找",[494],{"type":337,"value":492},{"type":332,"tag":359,"props":496,"children":500},{"className":497,"code":498,"language":499,"meta":325,"style":325},"language-js shiki shiki-themes slack-ochin slack-ochin ayu-dark","/**\n * @param {number[]} nums\n * @param {number} target\n * @return {number}\n */\nvar search = function (nums, t