:369,"tag":401,"props":416,"children":417},{},[418],{"type":374,"value":419},"输出：5\n",{"type":369,"tag":401,"props":421,"children":423},{"class":403,"line":422},3,[424],{"type":369,"tag":401,"props":425,"children":426},{},[427],{"type":374,"value":428},"解释：在第 2 天（股票价格 = 1）的时候买入，在第 5 天（股票价格 = 6）的时候卖出，最大利润 = 6-1 = 5 。\n",{"type":369,"tag":401,"props":430,"children":432},{"class":403,"line":431},4,[433],{"type":369,"tag":401,"props":434,"children":435},{},[436],{"type":374,"value":437},"注意利润不能是 7-1 = 6, 因为卖出价格需要大于买入价格；同时，你不能在买入前卖出股票。\n",{"type":369,"tag":370,"props":439,"children":440},{},[441],{"type":374,"value":442},"示例 2：",{"type":369,"tag":391,"props":444,"children":446},{"className":393,"code":445,"language":374,"meta":362,"style":362},"输入：prices = [7,6,4,3,1]\n输出：0\n解释：在这种情况下, 没有交易完成, 所以最大利润为 0。\n",[447],{"type":369,"tag":397,"props":448,"children":449},{"__ignoreMap":362},[450,458,466],{"type":369,"tag":401,"props":451,"children":452},{"class":403,"line":404},[453],{"type":369,"tag":401,"props":454,"children":455},{},[456],{"type":374,"value":457},"输入：prices = [7,6,4,3,1]\n",{"type":369,"tag":401,"props":459,"children":460},{"class":403,"line":413},[461],{"type":369,"tag":401,"props":462,"children":463},{},[464],{"type":374,"value":465},"输出：0\n",{"type":369,"tag":401,"props":467,"children":468},{"class":403,"line":422},[469],{"type":369,"tag":401,"props":470,"children":471},{},[472],{"type":374,"value":473},"解释：在这种情况下, 没有交易完成, 所以最大利润为 0。\n",{"type":369,"tag":370,"props":475,"children":476},{},[477],{"type":374,"value":478},"提示：",{"type":369,"tag":480,"props":481,"children":482},"ul",{},[483,489],{"type":369,"tag":484,"props":485,"children":486},"li",{},[487],{"type":374,"value":488},"1 \u003C= prices.length \u003C= 10^5",{"type":369,"tag":484,"props":490,"children":491},{},[492],{"type":374,"value":493},"0 \u003C= prices[i] \u003C= 10^4",{"type":369,"tag":495,"props":496,"children":498},"h2",{"id":497},"解题思路",[499],{"type":374,"value":497},{"type":369,"tag":495