363,"tag":395,"props":419,"children":420},{},[421],{"type":368,"value":422},"解释：在第 2 天（股票价格 = 1）的时候买入，在第 5 天（股票价格 = 6）的时候卖出，最大利润 = 6-1 = 5 。\n",{"type":363,"tag":395,"props":424,"children":426},{"class":397,"line":425},4,[427],{"type":363,"tag":395,"props":428,"children":429},{},[430],{"type":368,"value":431},"注意利润不能是 7-1 = 6, 因为卖出价格需要大于买入价格；同时，你不能在买入前卖出股票。\n",{"type":363,"tag":364,"props":433,"children":434},{},[435],{"type":368,"value":436},"示例 2：",{"type":363,"tag":385,"props":438,"children":440},{"className":387,"code":439,"language":368,"meta":356,"style":356},"输入：prices = [7,6,4,3,1]\n输出：0\n解释：在这种情况下, 没有交易完成, 所以最大利润为 0。\n",[441],{"type":363,"tag":391,"props":442,"children":443},{"__ignoreMap":356},[444,452,460],{"type":363,"tag":395,"props":445,"children":446},{"class":397,"line":398},[447],{"type":363,"tag":395,"props":448,"children":449},{},[450],{"type":368,"value":451},"输入：prices = [7,6,4,3,1]\n",{"type":363,"tag":395,"props":453,"children":454},{"class":397,"line":407},[455],{"type":363,"tag":395,"props":456,"children":457},{},[458],{"type":368,"value":459},"输出：0\n",{"type":363,"tag":395,"props":461,"children":462},{"class":397,"line":416},[463],{"type":363,"tag":395,"props":464,"children":465},{},[466],{"type":368,"value":467},"解释：在这种情况下, 没有交易完成, 所以最大利润为 0。\n",{"type":363,"tag":364,"props":469,"children":470},{},[471],{"type":368,"value":472},"提示：",{"type":363,"tag":474,"props":475,"children":476},"ul",{},[477,483],{"type":363,"tag":478,"props":479,"children":480},"li",{},[481],{"type":368,"value":482},"1 \u003C= prices.length \u003C= 10^5",{"type":363,"tag":478,"props":484,"children":485},{},[486],{"type":368,"value":487},"0 \u003C= prices[i] \u003C= 10^4",{"type":363,"tag":489,"props":490,"children":492},"h2",{"id":491},"解题思路",[493],{"type":368,"value":491},{"type":363,"tag":489,"props":495,"children":497},{"id":496},"实现",[498],{"type":368,"value":496},{"type":363,"tag":500,"props":501,"children":503},"h3",{"id":502},"动态规划",[504],{"type":368,"value":50